import("//Source/WebCore/web_core.gni")
import("//build/config/perl/perl.gni")

declare_args() {
  show_bindings_generation_progress = false
}

template("generate_bindings") {
  perl(target_name) {
    forward_variables_from(invoker,
                           [
                             "deps",
                             "inputs",
                             "outputs",
                           ])

    script = "bindings/scripts/generate-bindings-all.pl"

    inputs += [
      "bindings/scripts/generate-bindings.pl",
      "//Source/WTF/Scripts/Preferences/UnifiedWebPreferences.yaml",
    ]

    inputs += scripts_bindings

    deps += [ "//build:cmake_buildflags" ]

    idl_files_list = "$target_gen_dir/idl_files_$target_name.tmp"
    write_file(idl_files_list,
               rebase_path(invoker.inputs, root_build_dir),
               "list lines")

    pp_idl_files_list = "$target_gen_dir/pp_idl_files_$target_name.tmp"
    pp_idl_files = []
    if (defined(invoker.pp_idl_files)) {
      pp_idl_files += invoker.pp_idl_files
    }
    write_file(pp_idl_files_list,
               rebase_path(pp_idl_files, root_build_dir),
               "list lines")

    included_idl_files_list =
        "$target_gen_dir/included_idl_files_$target_name.tmp"
    included_idl_files = invoker.inputs
    if (defined(invoker.pp_included_files)) {
      included_idl_files += invoker.pp_included_files
    }
    write_file(included_idl_files_list,
               rebase_path(included_idl_files, root_build_dir),
               "list lines")

    args = [
      "--defines",
      string_join(" ", invoker.features),
      "--generator",
      invoker.generator,
      "--outputDir",
      rebase_path(invoker.output_dir, root_build_dir),
      "--idlFilesList",
      rebase_path(idl_files_list, root_build_dir),
      "--idlFileNamesList",
      rebase_path(included_idl_files_list, root_build_dir),
      "--ppIDLFilesList",
      rebase_path(pp_idl_files_list, root_build_dir),
      "--preprocessor",
      rebase_path(invoker.preprocessor, root_build_dir),
      "--idlAttributesFile",
      rebase_path("bindings/scripts/IDLAttributes.json", root_build_dir),
    ]

    if (defined(invoker.supplemental_depfile)) {
      args += [
        "--supplementalDependencyFile",
        rebase_path(invoker.supplemental_depfile, root_build_dir),
      ]
    }

    process_count =
        exec_script("//build/toolchain/get_cpu_count.py", [], "value")
    args += [
      "--numOfJobs",
      "$process_count",
    ]

    if (defined(invoker.pp_extra_output)) {
      foreach(i, invoker.pp_extra_output) {
        args += [
          "--ppExtraOutput",
          i,
        ]
      }
    }

    if (defined(invoker.pp_extra_args)) {
      foreach(i, invoker.pp_extra_args) {
        args += [
          "--ppExtraArgs",
          i,
        ]
      }
    }

    if (show_bindings_generation_progress) {
      args += [ "--showProgress" ]
    }
  }
}

